{
  "_version": "1.76.0",
  "sap.app": {
    "id": "sapui5task2",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },

    "dataSources": {
      "odataV2": {
        "uri": "/mockServerV2/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadataV2.xml"
        }
      },
      "odataV4": {
        "uri": "/mockServerV4/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0",
          "localUri": "localService/metadataV4.xml"
        }
      }
    },

    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.3",
      "toolsId": "ebe3b43c-ff9b-48be-95a2-4fc6e1b9e632"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.142.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sapui5task2.i18n.i18n"
        }
      },
      "odataV2": {
        "dataSource": "odataV2",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "odataV4": {
        "dataSource": "odataV4",
        "type": "sap.ui.model.odata.v4.ODataModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "sapui5task2.view",
        "async": true,
        "viewPath": "sapui5task2.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "",
          "target": "TargetMain"
        },
        {
          "name": "jsonTab",
          "pattern": "tab/json",
          "target": ["TargetMain", "TargetJSON"]
        },
        {
          "name": "odataV2Tab",
          "pattern": "tab/odataV2",
          "target": ["TargetMain", "TargetODataV2"]
        },
        {
          "name": "odataV4Tab",
          "pattern": "tab/odataV4",
          "target": ["TargetMain", "TargetODataV4"]
        },
        {
          "name": "productDetail",
          "pattern": "Product/{ProductID}",
          "target": ["TargetProduct"]
        }
      ],
      "targets": {
        "TargetMain": {
          "name": "Main",
          "id": "Main"
        },
        "TargetJSON": {
          "parent": "TargetMain",
          "name": "JSON",
          "id": "JSONView",
          "controlId": "jsonTab",
          "controlAggregation": "content"
        },
        "TargetODataV2": {
          "parent": "TargetMain",
          "name": "oDataV2",
          "id": "oDataV2View",
          "controlId": "odataTabV2",
          "controlAggregation": "content"
        },
        "TargetODataV4": {
          "parent": "TargetMain",
          "name": "oDataV4",
          "id": "oDataV4View",
          "controlId": "odataTabV4",
          "controlAggregation": "content"
        },
        "TargetProduct": {
          "id": "Product",
          "name": "Product"
        }
      }
    },
    "rootView": {
      "viewName": "sapui5task2.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
